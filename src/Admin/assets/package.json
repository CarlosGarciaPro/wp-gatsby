{
	"name": "wp-gatsby-preview-client",
	"version": "1.0.0",
	"description": "A small preview client script for Gatsby Preview with WordPress",
	"main": "index.js",
	"author": "Tyler Barnes <tylerdbarnes@gmail.com>",
	"license": "MIT",
	"devDependencies": {
		"@babel/cli": "^7.6.4",
		"@babel/core": "^7.6.4",
		"@babel/preset-env": "^7.12.1",
		"babel-preset-minify": "^0.5.1",
		"core-js": "2",
		"cssnano": "^4.1.10",
		"postcss": "^8.1.4",
		"postcss-cli": "^8.1.0",
		"postcss-preset-env": "^6.7.0"
	},
	"scripts": {
		"build-js": "babel src --out-dir dist --ignore **/__tests__ --extensions \".ts,.js\"",
		"watch-js": "babel -w src --out-dir dist --ignore **/__tests__ --verbose --extensions \".ts,.js\"",
		"build-css": "postcss src/styles.scss --output dist/styles.css --verbose",
		"watch-css": "postcss src/styles.scss --output dist/styles.css --verbose --watch"
	},
	"browserslist": [
		"> 0.25%",
		"not dead",
		"last 2 versions"
	]
}
